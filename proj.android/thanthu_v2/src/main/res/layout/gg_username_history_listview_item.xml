<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_gravity="center_horizontal"
    android:background="@drawable/gg_shape_corner_outline"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:minHeight="36dp">
    <TextView
        android:textSize="@dimen/gg_sixteen_sp"
        android:textColor="@color/gg_black"
        android:layout_gravity="center_vertical"
        android:id="@+id/gg_tv_username"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="8dp"/>
    <ImageView
        android:layout_gravity="center"
        android:id="@+id/gg_iv_name_clear"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginRight="8dp"
        android:src="@drawable/gg_icon_input_clear"/>
</FrameLayout>
