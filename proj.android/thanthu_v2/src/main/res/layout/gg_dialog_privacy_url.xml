<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@drawable/gg_shape_dialog_conner"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <WebView
        android:id="@+id/gg_webview_dialog_privacy"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginLeft="3dp"
        android:layout_marginTop="3dp"
        android:layout_marginRight="3dp"
        android:layout_weight="6"/>
    <LinearLayout
        android:orientation="horizontal"
        android:background="@drawable/gg_shape_dialog_conner_button"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
        <Button
            android:textColor="@color/gg_ticket_color_666666"
            android:id="@+id/gg_button_refuse"
            android:background="@drawable/gg_shape_dialog_conner_button_refuse"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"/>
        <Button
            android:textColor="@color/gg_white"
            android:id="@+id/gg_button_agree"
            android:background="@drawable/gg_shape_dialog_conner_button_agree"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"/>
    </LinearLayout>
</LinearLayout>
